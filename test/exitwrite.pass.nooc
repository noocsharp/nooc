let write proc(i64, str, i64) (i64) = proc(fd i64, string str, len i64) (out i64) {
	out = syscall(1, fd, string, len)
	return
}


let exit proc(i64) = proc(code i64) {
	syscall(60, code)
}

let main proc() = proc() {
	let ret i64 = 0
	ret = write(1, "hello\n", 6)
	if > ret 0 {
		exit(0)
	} else {
		exit(1)
	}
}
